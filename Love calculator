yourName = prompt("What is your name");
hisName = prompt("What is his/her name");
Sex= prompt("Male or Female");
Male = "him";
Female = "her";

yourName = Math.random()*101;
hisName= Math.random()*101;
result = (yourName + hisName)/2;
result=Math.floor(result);
alert ("You are " + result + "% compatible with " + (Sex === "Male" ? Male:Female));
