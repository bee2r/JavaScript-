var yourName = prompt("What is your name");
var hisName = prompt("What is his/her name");
var Sex= prompt("Male or Female");
var Male = "him";
var Female = "her";

yourName = Math.random()*101;
hisName= Math.random()*101;
var result = (yourName + hisName)/2;
result=Math.floor(result);
console.log("You are " + result + "% compatible with " + (Sex === "Male" ? Male:Female));
